#include <iostream>
using namespace std ;

void swap (int *x, int *y){
  int temp; 
  temp = *x;
  *x=*y;
  *y=temp;
}

int main() {
  int x,y;
  cout << "Please enter x and y\n";
  cin >> x >> y; 
  cout << "before swap x: "<< x << " and than "<< y<<endl;
  swap(&x,&y);
  cout << "after swap x: "<< x << " and than "<< y<<endl;
}