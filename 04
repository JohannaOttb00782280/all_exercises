#include <iostream>
using namespace std;

int floyd(int n, int a = 1){
  for (int i = 1; i<=n;i++) {

    for (int c = 1; c <= i; c++){
      cout <<a<< " ";
      a +=1;
    }
    cout << endl;
  }
  return 0;
}

int main() {
  int n;
  cout <<"How many lines do you want?: "; cin>>n; 
  floyd(n);
}